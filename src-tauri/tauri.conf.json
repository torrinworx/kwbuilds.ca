{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "KWBuilds.ca",
  "version": "1.0.0",
  "identifier": "ca.kwbuilds",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build:web",
    "frontendDist": "../build/dist",
    "devUrl": "http://localhost:3002"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
        {
          "title": "KWBuilds",
          "width": 800,
          "height": 600
        }
      ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
